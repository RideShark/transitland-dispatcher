<dl class="dl-horizontal">
  <dt>Created at</dt>
  <dd>{{model.created_at}}</dd>
  <dt>Updated at</dt>
  <dd>{{model.updated_at}}</dd>
</dl>

{{service-schedule-chart data=model.json.scheduled_service}}

<h4>Feed Statistics</h4>
<table class="table table-hover">
  <thead>
    <tr>
      <th>Column</th>
      <th>Rows</th>
      <th>Unique</th>
      <th>Min</th>
      <th>Max</th>
    </tr>
  </thead>
{{#each-in model.json.statistics as |key value|}}
  <tr><td colspan="5" style="border-bottom: solid 1px black">{{key}}</td></tr>
  {{#each-in value as |column stats|}}
    <tr>
      <td>{{column}}</td>
      <td>{{stats.total}}</td>
      <td>{{stats.unique}}</td>
      <td>{{stats.min}}</td>
      <td>{{stats.max}}</td>
    </tr>
  {{/each-in}}
{{/each-in}}
</table>


<h4>Service</h4>

<table class="table table-hover">
  <thead>
    <tr>
      <th>Date</th>
      <th>Service (seconds)</th>
    </tr>
  </thead>
  {{#each-in model.json.scheduled_service as |date value|}}
    <tr>
      <td>{{date}}</td>
      <td>{{value}}</td>
    </tr>
  {{/each-in}}
</table>
