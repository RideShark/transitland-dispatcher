<div class="container has-leaflet-map">
  <div class="row has-leaflet-map">
    <div class="col-md-3">
      <h3>Issues</h3>
      {{issue-table model=model issueClicked="issueClicked"}}
    </div>
    <div class="col-md-9 has-leaflet-map">
      {{issue-details classNames='issue-details' issue=model.selectedIssue}}
      {{edit-map model=model}}
    </div>
  </div>
</div>
